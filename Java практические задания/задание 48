import java.util.HashSet;

public class Main {
    public static void main(String[] args) {
        int x = 5;

        HashSet<Integer> set = new HashSet<>();
        set.add(1);

        for (int i = 2; i <= x; i++) {
            int y = 2 * i + 1;
            int z = 3 * i + 1;
            set.add(y);
            set.add(z);
        }

        System.out.println(set);
    }
}
