import java.util.Scanner;
public class Main {
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        System.out.println("Введите два числа:");
        int a = scan.nextInt();
        int b = scan.nextInt();

        for(int i = 1;i <= 10;i++) {
            int value = a + (i - 1) * b;
            System.out.println(value * value );
        }
    }
}
